        "@types/mime-types": "^2.1.0",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.12.21",
        "autoprefixer": "^9.4.7",
        "awesome-typescript-loader": "^5.2.1",
        "clean-webpack-plugin": "^1.0.1",
        "copy-webpack-plugin": "^4.6.0",
        "css-loader": "^2.1.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^3.0.1",
        "html-loader": "^0.5.5",
        "mini-css-extract-plugin": "^0.5.0",
        "mocha": "^5.2.0",
        "node-sass": "^4.11.0",
        "path": "^0.12.7",
        "postcss-loader": "^3.0.0",
        "raw-loader": "^1.0.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^1.2.2",
        "tslint": "^5.12.1",
        "typescript": "3.1.6",
        "url-loader": "^1.1.2",
        "webpack": "^4.29.3",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.1.14",
        "webpack-merge": "^4.2.1"
