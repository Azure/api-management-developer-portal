        "mini-css-extract-plugin": "^2.6.0",
        "mocha": "^9.2.2",
        "path": "^0.12.7",
        "postcss-loader": "^6.2.1",
        "puppeteer": "13.5.1",
        "querystring-es3": "^0.2.1",
