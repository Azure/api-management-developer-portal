{
    "name": "apim-developer-portal",
    "version": "0.0.20",
    "description": "API management developer portal",
    "author": "Microsoft",
    "license": "MIT",
    "keywords": [
        "azure"
    ],
    "engines": {
        "node": ">=10.12"
    },
    "scripts": {
        "start": "webpack-dev-server --open --config webpack.develop.js",
        "test": "node node_modules/mocha/bin/_mocha -r mocha.js **/*.spec.ts",
        "publish": "npm run build-publisher && node ./dist/publisher/index.js",
        "build-designer": "webpack --config webpack.build.js",
        "build-publisher": "webpack --config webpack.publisher.js",
        "build-function": "webpack --config webpack.function.js",
        "build-runner": "webpack --config webpack.publish.runner.js",
        "deploy-editors": "npm run build-designer && npm run build-runner && node ./scripts/editorsToBlob.js",
        "deploy-function": "npm run build-function && cd dist/function && func azure functionapp publish < function app name >",
        "postinstall": "npm rebuild node-sass"
    },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/google-maps": "^3.2.1",
        "@types/knockout": "^3.4.66",
        "@types/knockout.mapping": "^2.0.34",
        "@types/knockout.validation": "0.0.36",
        "@types/lodash": "^4.14.134",
        "@types/mime-types": "^2.1.0",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.0.8",
        "autoprefixer": "^9.6.0",
        "awesome-typescript-loader": "^5.2.1",
        "azure-storage": "^2.10.3",
        "chai": "^4.2.0",
        "clean-webpack-plugin": "^2.0.2",
        "copy-webpack-plugin": "^5.0.3",
        "css-loader": "^3.0.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^4.0.0",
        "html-loader": "^0.5.5",
        "mini-css-extract-plugin": "^0.7.0",
        "mocha": "^6.1.4",
        "node-sass": "^4.12.0",
        "path": "^0.12.7",
        "postcss-loader": "^3.0.0",
        "raw-loader": "^3.0.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^1.3.0",
        "ts-node": "^8.2.0",
        "tslint": "^5.17.0",
        "typescript": "3.5.2",
        "url-loader": "^2.0.0",
        "webpack": "^4.34.0",
        "webpack-cli": "^3.3.4",
        "webpack-dev-server": "^3.7.1",
        "webpack-merge": "^4.2.1"
    },
    "dependencies": {
        "@paperbits/azure": "0.1.132",
        "@paperbits/common": "0.1.132",
        "@paperbits/core": "0.1.132",
        "@paperbits/prosemirror": "0.1.132",
        "@paperbits/styles": "0.1.132",
        "@webcomponents/webcomponentsjs": "^2.2.10",
        "core-js": "^3.1.4",
        "domino": "^2.1.3",
        "dotenv": "^8.0.0",
        "file-saver": "^2.0.2",
        "flatten": "^1.0.2",
        "google-maps": "^3.3.0",
        "js-beautify": "^1.10.0",
        "knockout": "^3.5.0",
        "knockout-mapping": "^2.6.0",
        "knockout.validation": "^2.0.3",
        "liquidjs": "^8.2.3",
        "lodash": "^4.17.14",
        "lunr": "^2.3.6",
        "moment": "^2.24.0",
        "prismjs": "^1.16.0",
        "remark": "^10.0.1",
        "remark-html": "^9.0.1",
        "slick": "^1.12.2",
        "specificity": "^0.4.1",
        "xhr2": "^0.2.0"
    }
}
